msgid "Username"
msgstr "Nom d'utilisateur"

#: pretalx/orga/forms/event.py:284 pretalx/orga/templates/orga/admin.html:56
#: pretalx/person/forms.py:30 pretalx/person/forms.py:34
msgid "Password"
msgstr "Mot de passe"

